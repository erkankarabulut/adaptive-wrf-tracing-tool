<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.GridPane?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ComboBox?>
<?import java.lang.String?>
<?import javafx.collections.FXCollections?>
<?import javafx.scene.layout.AnchorPane?>
<AnchorPane fx:controller="main.java.controller.MainController"
            xmlns:fx="http://javafx.com/fxml">

    <Label layoutX="10.0" layoutY="10.0" prefHeight="21.0" prefWidth="279.0" text="Log File: " />
    <Label layoutX="10.0" layoutY="50.0" prefHeight="21.0" prefWidth="279.0" text="Algorithm: " />

    <Button fx:id="chooseLogFileButton" layoutX="100.0" layoutY="10.0" prefWidth="180.0" mnemonicParsing="false" text="Select File" />
    <Button fx:id="runButton" layoutX="10.0" layoutY="90.0" prefWidth="270" mnemonicParsing="false" text="Run" />

    <ComboBox fx:id="selectAlgorithmComboBox" layoutX="100.0" layoutY="50.0" prefWidth="180.0" promptText="Choose Algorithm">
        <items>
            <FXCollections fx:factory="observableArrayList">
                <String fx:value="Logistic Regression" />
                <String fx:value="Bayesian Linear Regression" />
                <String fx:value="Least Absolute Deviation Regression" />
                <String fx:value="Winnow Algorithm" />
            </FXCollections>
        </items>
    </ComboBox>

</AnchorPane>